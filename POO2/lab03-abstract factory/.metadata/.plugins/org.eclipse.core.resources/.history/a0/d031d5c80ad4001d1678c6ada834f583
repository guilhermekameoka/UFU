package factory;

public class CarroChevrolet extends Carro {
    public CarroChevrolet(FabricaDeCarros fabrica) {
        super(fabrica);
    }

    public String getDescricao() {
        return "Carro Chevrolet com " + super.motor.getDescricao() + " e " + super.acessorios.getDescricao();
    }
}

