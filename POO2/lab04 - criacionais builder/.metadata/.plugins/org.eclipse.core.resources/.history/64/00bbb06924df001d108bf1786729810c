package builder;

public class Personagem {
	   private String nome;
	   private String profissao;
	   private String arma;

	   public Personagem(String nome, String profissao, String arma) {
	      this.nome = nome;
	      this.profissao = profissao;
	      this.arma = arma;
	   }

	   public String getNome() {
	      return nome;
	   }

	   public String getProfissao() {
	      return profissao;
	   }

	   public String getArma() {
	      return arma;
	   }
	}
